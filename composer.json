{
    "name": "codica-studio/userdataexport",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codicastudio/userdataexport",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-zip": "*",
        "illuminate/filesystem": "^8.0",
        "illuminate/queue": "^8.0",
        "illuminate/support": "^8.0",
        "nesbot/carbon": "^2.14",
        "spatie/temporary-directory": "^1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\PersonalDataExport\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\PersonalDataExport\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\PersonalDataExport\\PersonalDataExportServiceProvider"
            ]
        }
    }
}
